{
	"$schema": "./node_modules/@angular/service-worker/config/schema.json",
	"index": "/index.html",
	"assetGroups": [
		{
			"name": "app",
			"installMode": "prefetch",
			"resources": {
				"files": [
					"/config/**/*",
					"/i18n/**/*",
					"/favicon.ico",
					"/index.html",
					"/manifest.webmanifest",
					"/*.css",
					"/*.js"
				]
			}
		},
		{
			"name": "assets",
			"installMode": "lazy",
			"updateMode": "prefetch",
			"resources": {
				"files": [
					"/assets/**/*",
					"/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani|pdf)"
				],
				"urls": [
					"https://raw.githubusercontent.com/*",
					"https://fonts.googleapis.com/*"
				]
			}
		}
	],
	"dataGroups": [
		{
			"name": "api",
			"urls": [
				"https://disease.sh/*"
			],
			"cacheConfig": {
				"maxSize": 100,
				"maxAge": "5m",
				"timeout": "5s",
				"strategy": "freshness"
			}
		}
	]
}
