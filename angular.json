{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"cli": {
		"packageManager": "npm"
	},
	"newProjectRoot": "projects",
	"projects": {
		"covid": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "",
			"sourceRoot": "src",
			"prefix": "covid",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/covid",
						"index": "src/index.html",
						"main": "src/main.ts",
						"polyfills": "src/polyfills.ts",
						"tsConfig": "tsconfig.app.json",
						"aot": true,
						"assets": [
							"src/favicon.png",
							"src/assets",
							"src/i18n",
							{
								"glob": "**/*",
								"input": "src/environments/configs",
								"output": "/configs/"
							},
							"src/manifest.webmanifest"
						],
						"styles": [
							"src/styles.scss"
						],
						"scripts": [
							"node_modules/apexcharts/dist/apexcharts.min.js"
						]
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb",
									"maximumError": "10kb"
								}
							],
							"serviceWorker": true,
							"ngswConfigPath": "ngsw-config.json"
						}
					}
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "covid:build"
					},
					"configurations": {
						"production": {
							"browserTarget": "covid:build:production"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "covid:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "src/test.ts",
						"polyfills": "src/polyfills.ts",
						"tsConfig": "tsconfig.spec.json",
						"karmaConfig": "karma.conf.js",
						"assets": [
							"src/favicon.png",
							"src/assets",
							"src/i18n",
							{
								"glob": "**/*",
								"input": "src/environments/configs",
								"output": "/configs/"
							},
							"src/manifest.webmanifest"
						],
						"styles": [
							"src/styles.scss"
						],
						"scripts": []
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [
							"tsconfig.app.json",
							"tsconfig.spec.json",
							"e2e/tsconfig.json"
						],
						"exclude": [
							"**/node_modules/**"
						]
					}
				},
				"e2e": {
					"builder": "@nrwl/cypress:cypress",
					"options": {
						"cypressConfig": "e2e/cypress.json",
						"tsConfig": "e2e/tsconfig.e2e.json",
						"devServerTarget": "covid:serve"
					},
					"configurations": {
						"production": {
							"devServerTarget": "covid:serve:production"
						}
					}
				}
			}
		},
		"charts": {
			"projectType": "library",
			"root": "projects/charts",
			"sourceRoot": "projects/charts/src",
			"prefix": "covid",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-ng-packagr:build",
					"options": {
						"tsConfig": "projects/charts/tsconfig.lib.json",
						"project": "projects/charts/ng-package.json"
					},
					"configurations": {
						"production": {
							"tsConfig": "projects/charts/tsconfig.lib.prod.json"
						}
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/charts/src/test.ts",
						"tsConfig": "projects/charts/tsconfig.spec.json",
						"karmaConfig": "projects/charts/karma.conf.js"
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [
							"projects/charts/tsconfig.lib.json",
							"projects/charts/tsconfig.spec.json"
						],
						"exclude": [
							"**/node_modules/**"
						]
					}
				}
			}
		}
	},
	"defaultProject": "covid"
}
